<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.humdet">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconhumdet"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconhumdet"
        android:supportsRtl="true"
        android:theme="@style/Theme.GOZ">
        <!--<meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713"/>-->
        <activity
            android:name="com.humdet.NeuroTrainingActivity"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.humdet.DetailActivity"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.humdet.ResultSearchActivity"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.humdet.SearchActivity"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.humdet.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.humdet.MyService"
            android:enabled="true"
            android:exported="true"
            android:theme="@style/Theme.AppCompat" />
    </application>
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-feature android:name="android.hardware.camera2.full" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.ACTION_MANAGE_OVERLAY_PERMISSION" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

</manifest>